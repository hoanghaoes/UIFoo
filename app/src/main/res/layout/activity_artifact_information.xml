<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="artifactInformationVM" type="com.foo.app.modules.artifactinformation.data.viewmodel.ArtifactInformationVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearArtifactinform"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylebluegray_100">
        <LinearLayout android:gravity="end"
            android:id="@+id/linearIOSStatusBar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageMusic"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_28pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_music"
                tools:ignore="ContentDescription" tools:src="@drawable/img_music"/>
            <ImageView android:id="@+id/imageRightSide"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_216pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_66pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_rightside"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rightside"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearHeader"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageButton
                android:background="@drawable/rectangle_bg_bluegray_800_radius_17"
                android:id="@+id/btnVector"
                android:layout_height="@dimen/_34pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_34pxh"
                android:paddingHorizontal="@dimen/_10pxh"
                android:paddingVertical="@dimen/_10pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_vector" tools:ignore="ContentDescription"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtThngtin"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_70pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.66" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_6pxh"
                android:singleLine="true"
                android:text="@{artifactInformationVM.artifactInformationModel.txtThngtin}"
                style="@style/txtInterbold24" tools:text="@string/lbl_th_ng_tin"/>
            <ImageView android:id="@+id/imageHome"
                android:layout_height="@dimen/_25pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_80pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_home"
                tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameImg"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_199pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_22pxv" android:layout_width="@dimen/_300pxh">
            <ImageView android:id="@+id/imageImageTwo"
                android:layout_gravity="start"
                android:layout_height="@dimen/_199pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_10pxh}"
                imageRes="@{@drawable/img_image2}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image2"/>
            <LinearLayout android:id="@+id/linearName"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylecornerRadius_7">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtKhuVnCc"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_59pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_marginTop="@dimen/_156pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{artifactInformationVM.artifactInformationModel.txtKhuVnCc}"
                    style="@style/txtBungeeInlineregular24" tools:text="@string/lbl_khu_v_n_c_c"/>
            </LinearLayout>
        </FrameLayout>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtDescription"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_39pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_6pxh"
            android:text="@{artifactInformationVM.artifactInformationModel.txtDescription}"
            style="@style/txtArialmt14" tools:text="@string/msg_khu_v_n_c_c_tr"/>
    </LinearLayout>
</layout>
