<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="doiThuongOneVM" type="com.foo.app.modules.doithuongone.data.viewmodel.DoiThuongOneVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnmusic"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_17pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearDoithuongOne"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearIOSStatusBar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageMusic"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_music"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_music"/>
                <ImageView android:id="@+id/imageRightSide"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_216pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_66pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rightside"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rightside"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearHeader"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtIthng"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_77pxh"
                    android:layout_weight="0.94"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{doiThuongOneVM.doiThuongOneModel.txtIthng}"
                    style="@style/txtInterbold24" tools:text="@string/lbl_i_th_ng"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearContainer"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_100">
                <FrameLayout android:id="@+id/frameHeader"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_256pxv"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_38pxh" android:layout_width="@dimen/_284pxh">
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_40"
                        android:id="@+id/viewRectangle247"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_235pxv"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_283pxh"/>
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_183pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_38pxh"
                        android:layout_marginStart="@dimen/_38pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="@dimen/_201pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_addressbro1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_addressbro1"/>
                    <LinearLayout android:id="@+id/linearRow"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylebluegray_500cornerRadius">
                        <LinearLayout android:id="@+id/linearRowzipcode"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_34pxv"
                            android:layout_marginStart="@dimen/_21pxh"
                            android:layout_marginTop="@dimen/_16pxv"
                            android:layout_weight="0.37"
                            android:layout_width="0dp"
                            android:orientation="horizontal" style="@style/groupStylecornerRadius">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtZipcode"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.83"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{doiThuongOneVM.doiThuongOneModel.txtZipcode}"
                                style="@style/txtInterbold16" tools:text="@string/lbl_3600"/>
                            <ImageView android:id="@+id/imageIcon"
                                android:layout_height="@dimen/_14pxh"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginEnd="@dimen/_3pxh"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="@dimen/_9pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_icon_white_a700"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_icon_white_a700"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRowqui"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_36pxv"
                            android:layout_marginEnd="@dimen/_20pxh"
                            android:layout_marginStart="@dimen/_69pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_weight="0.63"
                            android:layout_width="0dp"
                            android:orientation="horizontal" style="@style/groupStylecornerRadius">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtQui"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.83"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{doiThuongOneVM.doiThuongOneModel.txtQui}"
                                style="@style/txtInterbold16" tools:text="@string/lbl_qu_i"/>
                            <ImageView android:id="@+id/imageComputer"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_width="@dimen/_17pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_computer"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_computer"/>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:gravity="start"
                    android:id="@+id/recyclerGifts"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_11pxh}"
                    style="@style/groupStylewhite_A700cornerRadius"
                    tools:itemCount="7" tools:listitem="@layout/row_gifts"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
