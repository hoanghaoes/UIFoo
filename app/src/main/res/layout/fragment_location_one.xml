<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="locationOneVM" type="com.foo.app.modules.locationone.data.viewmodel.LocationOneVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStack"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <FrameLayout android:id="@+id/frameStackmusic"
            android:layout_gravity="start"
            android:layout_height="match_parent" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumnupload"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearColumnmusic"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearIOSStatusBar"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageMusic"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_music"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_music"/>
                        <ImageView android:id="@+id/imageRightSide"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_216pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_66pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_rightside"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rightside"/>
                    </LinearLayout>
                    <EditText
                        android:background="@drawable/rectangle_bg_white_a700_radius_20"
                        android:drawableEnd="@drawable/img_icon"
                        android:drawablePadding="@dimen/_30pxh"
                        android:ellipsize="end"
                        android:hint="@string/lbl_nh_p_v_tr"
                        android:id="@+id/etTmkim"
                        android:inputType="text"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_16pxh"
                        android:paddingStart="@dimen/_20pxh"
                        android:paddingTop="@dimen/_11pxv"
                        android:singleLine="true"
                        android:text="@={locationOneVM.locationOneModel.etTmkimValue}"
                        android:textColorHint="@color/black_900_87"
                        style="@style/etSolidRounded_1" tools:ignore="Autofill"/>
                    <ImageView android:id="@+id/imageStar"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_8pxh"
                        android:layout_marginEnd="@dimen/_125pxh"
                        android:layout_marginStart="@dimen/_125pxh"
                        android:layout_marginTop="@dimen/_257pxv"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                    <ImageView android:id="@+id/imageIcon"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_marginEnd="@dimen/_130pxh"
                        android:layout_marginStart="@dimen/_130pxh"
                        android:layout_marginTop="@dimen/_74pxv"
                        android:layout_width="@dimen/_18pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_icon_red_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_icon_red_a700"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageBasemapimage"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_715pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_basemapimage}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_basemapimage"/>
        </FrameLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/bluegray_800"
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|end"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_104pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_104pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_60pxh"
            app:srcCompat="@drawable/img_group1" app:tint="@color/gray_50"/>
    </FrameLayout>
</layout>
